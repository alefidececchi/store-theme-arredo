{
    "tab-list#global__plp": {
        "children": [
            "tab-list.item#global__pdp--product-description",
            "tab-list.item#global__pdp--reviews-and-ratings",
            "tab-list.item#global__pdp--questions-and-answers"
        ],
        "props": {
            "blockClass": "global__plp"
        },
        "title": "Global tab list layout"
    },
    "tab-list.item#global__pdp--product-description": {
        "props": {
            "blockClass": [
                "global__pdp--list-item",
                "global__pdp--product-description"
