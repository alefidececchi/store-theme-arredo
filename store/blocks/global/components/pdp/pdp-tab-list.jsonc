            "tabId": "product-description"
        },
        "title": "Global tab list item"
    },
    "tab-list.item#global__pdp--reviews-and-ratings": {
        "props": {
            "blockClass": [
                "global__pdp--list-item",
                "global__pdp--reviews-and-ratings"
            ],
            "label": "Reseñas",
            "tabId": "reviews-and-ratings"
        },
        "title": "Global tab list item"
    },
    "tab-list.item#global__pdp--questions-and-answers": {
        "props": {
            "blockClass": [
                "global__pdp--list-item",
                "global__pdp--questions-and-answers"
            ],
            "label": "Preguntas y respuestas",
            "tabId": "questions-and-answers"
        },
        "title": "Global tab list item"
    }
