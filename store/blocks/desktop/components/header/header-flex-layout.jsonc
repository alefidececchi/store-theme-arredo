{
    "flex-layout.row#desktop__header--row1": {
        "children": [
            "flex-layout.col#desktop__header--link",
            "header-force-center#global__logo--container",
            "flex-layout.col#desktop__header--container"
        ],
        "props": {
            "blockClass": "desktop__header--row1",
            "fullWidth": true,
            "horizontalAlign": "center",
            "preventHorizontalStretch": true,
            "preventVerticalStretch": true
        },
        "title": "Desktop sticky header row 1"
    },
    "flex-layout.row#desktop__header--row2": {
        "children": [
            "vtex.menu@2.x:menu#desktop__websites"
        ],
        "props": {
            "blockClass": "desktop__header--row2",
            "borderWidth": 5,
            "colSinzing": "auto",
            "colJustify": "between",
            "fullWidth": true,
            "horizontalAlign": "center",
            "preventHorizontalStretch": true,
            "preventVerticalStretch": true
        },
        "title": "Desktop sticky header row 2"
    },
    "flex-layout.col#desktop__header--link": {
        "children": [
            "link#global__header--link"
        ],
        "props": {
            "blockClass": "desktop__header--link",
            "horizontalAlign": "center",
            "verticalAlign": "middle",
            "width": "100%"
        },
        "title": "Container chat link"
    },
    "flex-layout.col#desktop__header--container": {
        "children": [
            "flex-layout.row#header__container--col-3"
        ],
        "props": {
            "blockClass": "desktop__header--container",
            "horizontalAlign": "center",
            "verticalAlign": "middle",
            "width": "100%"
        },
        "title": "Searchbar, login and cart (colum-container)"
    },
    "flex-layout.row#header__container--col-3": {
        "children": [
            "search-bar#global__header--searchbar",
            "login#desktop",
            "minicart.v2"
        ],
        "props": {
            "blockClass": "header__container--col-3",
            "colSinzing": "auto",
            "horizontalAlign": "center",
            "preventHorizontalStretch": true,
            "preventVerticalStretch": true
        },
        "title": "Searchbar, login and cart (row-container)"
    },

    "flex-layout.row#desktop__container--department1": {
        "children": [
            "flex-layout.col#desktop__container--submenu",
            "flex-layout.col#desktop__container--image1"
        ],
        "props": {
            "blockClass": "desktop__container--department1",
            "fullWidth": true,
            "horizontalAlign": "center",
            "preventHorizontalStretch": true,
            "preventVerticalStretch": true
        },
        "title": "Desktop row container department 1"
    },
    "flex-layout.col#desktop__container--submenu": {
        "children": [
            "vtex.menu@2.x:menu#submenu__bedclothes",
            "link#desktop__department1"
        ],
        "props": {
            "blockClass": "desktop__container--submenu",
            "horizontalAlign": "left",
            "preventVerticalStretch": true,
            "verticalAlign": "middle",
            "width": "30%"
        },
        "title": "Container submenu"
    },
    "flex-layout.col#desktop__container--image1": {
        "children": [
            "image#desktop__menu--department1"
        ],
        "props": {
            "blockClass": "desktop__container--image1",
            "horizontalAlign": "left",
            "preventVerticalStretch": true,
            "verticalAlign": "middle"
        },
        "title": "image department 1 container"
    },
    "flex-layout.col#desktop__container--img-measures": {
        "children": [
            "image#desktop__submenu--measures"
        ],
        "props": {
            "blockClass": "desktop__container--img-measures",
            "horizontalAlign": "left",
            "preventVerticalStretch": true,
            "verticalAlign": "middle"
        },
        "title": "Submenu image container"
    },
    "flex-layout.row#desktop__container--measures": {
        "children": [
            "vtex.menu@2.x:menu#desktop__block--measures",
            "flex-layout.col#desktop__container--img-measures"
        ],
        "props": {
            "blockClass": "desktop__container--measures",
            "fullWidth": true,
            "preventVerticalStretch": true,
            "horizontalAlign": "left"
        },
        "title": "Container submenu 'measures'"
    }
}